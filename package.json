{
	"name": "logoscope",
	"version": "0.1.5",
	"publisher": "danielvartan",
	"license": "SEE LICENSE IN LICENSE.md",
	"displayName": "LogoScope",
	"description": "NetLogo Syntax Highlighting",
	"icon": "images/icon.png",
	"author": {
		"name": "Daniel Vartanian",
		"email": "danielvartan@proton.me",
		"url": "https://danielvartan.com"
	},
	"categories": [
		"Formatters",
		"Programming Languages"
	],
	"keywords": [
		"NetLogo",
		"NetLogo Language",
		"NetLogo Syntax",
		"NetLogo Source File",
		"NetLogo Model",
		"NetLogo XML Model",
		"Logo",
		"Logo Language",
		"Logo Syntax",
		"Syntax Highlighting",
		"Code Highlighting",
		"Programming Language Support",
		"Modeling",
		"Computational Modeling",
		"Ecological Modeling",
		"Agent-Based Modeling",
		"Individual-Based Modeling",
		"Multi-Agent Systems",
		"System Dynamics",
		"Complex Systems",
		"Complexity Science",
		"Generative Science",
		"Scientific Computing",
		"Computational Science",
		"Education"
	],
	"homepage": "https://github.com/danielvartan/logoscope",
	"repository": {
		"type": "git",
		"url": "https://github.com/danielvartan/logoscope"
	},
	"bugs": {
		"url": "https://github.com/danielvartan/logoscope/issues",
		"email": "danielvartan@proton.me"
	},
	"sponsor": {
		"url": "https://github.com/sponsors/danielvartan"
	},
	"main": "./out/extension.js",
	"activationEvents": [],
	"engines": {
		"vscode": "^1.102.0",
		"positron": "^2025.8.x"
	},
	"contributes": {
		"languages": [
			{
				"id": "netlogo",
				"aliases": [
					"NetLogo",
					"Netlogo",
					"netLogo",
					"netlogo"
				],
				"extensions": [
					".nls"
				],
				"configuration": "language-configuration.json"
			},
			{
				"id": "nlogo",
				"aliases": [
					"NetLogo Model"
				],
				"extensions": [
					".nlogo",
					".nlogo3d"
				],
				"configuration": "language-configuration.json"
			},
			{
				"id": "nlogox",
				"aliases": [
					"NetLogo XML Model"
				],
				"extensions": [
					".nlogox",
					".nlogox3d"
				],
				"configuration": "language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "netlogo",
				"scopeName": "source.netlogo",
				"path": "syntaxes/netlogo.json"
			},
			{
				"language": "nlogo",
				"scopeName": "source.nlogo",
				"path": "syntaxes/nlogo.json"
			},
			{
				"language": "nlogox",
				"scopeName": "source.nlogox",
				"path": "syntaxes/nlogox.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -watch -p ./",
		"test:grammar": "vscode-tmgrammar-test tests/*.nlogo",
		"pretest": "npm run compile && npm run lint && npm run test:grammar",
		"js-yaml-netlogo": "npx js-yaml syntaxes/netlogo.yaml > syntaxes/netlogo.json",
		"js-yaml-nlogo": "npx js-yaml syntaxes/nlogo.yaml > syntaxes/nlogo.json",
		"js-yaml-nlogox": "npx js-yaml syntaxes/nlogox.yaml > syntaxes/nlogox.json",
		"render-json": "npm run js-yaml-netlogo && npm run js-yaml-nlogo && npm run js-yaml-nlogox",
		"render-cff": "node --no-warnings js/render-cff.js"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.100.0",
		"eslint": "^9.13.0",
		"js-yaml": "^4.1.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0",
		"vscode-tmgrammar-test": "^0.1.3"
	},
	"dependencies": {
		"@posit-dev/positron": "^0.1.3",
		"fs": "^0.0.1-security",
		"path": "^0.12.7"
	}
}
