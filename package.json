{
	"name": "logoscope",
	"displayName": "LogoScope",
	"description": "NetLogo Syntax Highlight for Positron and Visual Studio Code",
	"version": "0.1.0",
	"publisher": "danielvartan",
	"repository": {
		"type": "git",
		"url": "https://github.com/danielvartan/logoscope"
	},
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.74.0",
		"positron": "^2025.6.x"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "^20",
		"@types/sinon": "^17.0.4",
		"@types/vscode": "^1.100.0",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.13.0",
		"glob": "^11.0.0",
		"js-yaml": "^4.1.0",
		"mocha": "^11.6.0",
		"sinon": "^20.0.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0"
	},
	"dependencies": {
		"@posit-dev/positron": "^0.1.3"
	},
	"contributes": {
		"languages": [
			{
				"id": "netlogo",
				"aliases": ["NetLogo"],
				"extensions": [".nlogo", ".nlogo3d", ".nls"],
				"configuration": "language-configuration.json"
			},
			{
				"id": "nlogox",
				"aliases": ["NetLogo XML"],
				"extensions": [".nlogox", ".nlogox3d"],
				"configuration": "language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "netlogo",
				"scopeName": "source.nlogo",
				"path": "syntaxes/nlogo.json"
			},
			{
				"language": "nlogox",
				"scopeName": "source.nlogox",
				"path": "syntaxes/nlogox.json"
			}
		]
	}
}
